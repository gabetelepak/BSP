if scr_pug_dis(6) &gt; 0{
if pug.speed &gt; 0{
direction = point_direction(x,y,pug.x,pug.y)+170+random(20);
speed = .5;}}}


if alarm[0] &gt; 0{
alarm[0] -= 1;}

if alarm[1] &gt; 0{
alarm[1] -= 1;}

if alarm[0] &lt;= 0{
image_speed = .5;
alarm[0] = 30 + random(10);

ix = floor(sx + random(30)-15);
iy = floor(sy + random(20)-15);
iz = floor(20 + random(5));
}

if bubble &gt; 0{
iz = floor(5 + random(5));}


//movement
if speed &lt; 2{
if distance_to_point(ix,iy) &gt; 2{
move_towards_point(ix,iy,.2);}}

if z &lt; iz{z += .1;}
if z &gt; iz{z -= .15;}


if alarm[1] &lt;= 0{
alarm[1] = 30 + random(30);
spr_offsetx = random(2)-1;
spr_offsety = random(2)-1;}


//get bubbled
if bubble = 0{
if instance_exists(obj_bubble){
opt = instance_nearest(x,y,obj_bubble);
if distance_to_point(opt.x,opt.y) &lt; 10 &amp;&amp; opt.sprite_index = spr_bubble_float{
with opt{instance_destroy();}
bubble = 1;
}}}

//get bit
if scr_pug_dis(2) &gt; 0{
if pug.sprite_index = pug.spr_bite || pug.sprite_index = pug.spr_bite_low{
if pug.pug_action = 3 &amp;&amp; pug.pug_action_proc = 0{
pug.pug_action_proc = 1;
pug.ate_firefly += 1;
instance_destroy();}}}

//get netted
if scr_pug_dis(8) &gt; 0{
if pug.sprite_index = pug.spr_bugnet_use{
if pug.pug_action = 5 &amp;&amp; pug.pug_action_proc = 0{
