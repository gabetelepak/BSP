draw_sprite(spr_select_portrait,0,sx+165+ix,sy+29+iy);}}

//p2
if g.selected_char[1] = i{
if g.bg_anim[i] &lt; 2{g.bg_anim[i]+= .25;}
draw_sprite(spr_blue_mouse_over_portrait,g.bg_anim[i],sx+165+ix,sy+29+iy);
if g.bg_anim[i] = 2 &amp;&amp; g.slow_blink = 1 &amp;&amp; g.selected_char_confirm[1] = 1{
draw_sprite(spr_select_portrait,0,sx+165+ix,sy+29+iy);}}

if (g.selected_char_confirm[0] = 1 &amp;&amp; g.selected_char[0] = i) || (g.selected_char_confirm[1] = 1 &amp;&amp; g.selected_char[1] = i){
draw_sprite(g.char_select[i],g.select_anim[i],sx+165+ix,sy+29+iy);}else{
draw_sprite(g.char_idle[i],g.idle_anim,sx+165+ix,sy+29+iy);}

//de animate the bg box for slot
if g.selected_char[0] != i &amp;&amp; g.selected_char[1] != i{
if g.bg_anim[i] &gt; 0{g.bg_anim[i]-= .25;}}

ix += 96;
if ix = 96*4{
