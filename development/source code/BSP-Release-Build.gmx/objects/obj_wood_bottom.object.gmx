if g.selected_char[0] &gt;= 0{
if g.char_head_roll[0] &lt; 130{
g.char_head_roll[0] += max(1,(135-g.char_head_roll[0])*.125);}
draw_sprite(g.char_big[g.selected_char[0]],0,sx+50-130+g.char_head_roll[0],sy+27);

draw_sprite(g.char_name[g.selected_char[0]],0,sx+50,sy-59);
draw_sprite(spr_player_token,0,sx+5,sy+62);
}else{
