move_towards_point(sx,sy,move_speed*.75);}
else{
speed = 0;
x = sx; 
y = sy;
sprite_index = spr_granny_sit;
image_index = 0;
image_speed = .25;
state = "start";}}


//attack
if state = "attack"{
speed = 0;
if target &gt; 0 &amp;&amp; instance_exists(target){
move_towards_point(target.x,target.y,move_speed);
sprite_index = spr_granny_walk_shoo;
image_speed = .2;

if scr_pug_dis(4) &gt; 0{
scr_sound(snd_granny_hit,g.sfx_volume);
speed = 0;
state = "swing";
sprite_index = spr_granny_walk_hit;
image_index = 0;
image_speed = .33;}

//dont get carried away with chase
if scr_dis(sx,sy,x,y) &gt; 120{
sprite_index = spr_granny_walk;
image_speed = .2;
state = "retreat";}

}}

//whack pug
if state = "swing"{
speed = 0;
if image_index &gt; image_number - 1{
if scr_pug_dis(12) &gt; 0{
if pug.hurt = 0 &amp;&amp; pug.sprite_index != pug.spr_stung{
pug.hurt = 2;}}
sprite_index = spr_granny_walk;
image_speed = .2;
state = "retreat";}}


//init cry
if state = "defend" &amp;&amp; cry = 1{
scr_sound(snd_granny_cry,g.sfx_volume);
