if abs(x-target.x) &gt; 12{
if x &lt; target.x &amp;&amp; place_free(x+24,y){hopx = x+24;hopy = y;}
if x &gt; target.x &amp;&amp; place_free(x-24,y){hopx = x-24;hopy = y;}}

if abs(y-target.y) &gt; 6{
if y &lt; target.y &amp;&amp; place_free(x,y+24){hopy = y+24;hopx = x;}
if y &gt; target.y &amp;&amp; place_free(x,y-24){hopy = y-24;hopx = x;}}

opt = instance_place(hopx,hopy, obj_frog);
if opt &gt; 0 &amp;&amp; instance_exists(opt){
if opt.float_alarm&lt;= 0{hopx = 0;hopy=0;}}


}}

//init hop
