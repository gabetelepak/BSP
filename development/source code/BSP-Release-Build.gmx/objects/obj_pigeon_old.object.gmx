if g.game_phase = 'park night'{
zbouncable = 0;

alarm[1] -= 1;
alarm[0] -= 1;
if alarm[0] &lt;= 0{
action = 0;
spawn = 0;}

if interact = ""{
spr_action = 0;



if action = 0{

//bark reaction
if scr_pug_dis(64) {
if pug.sprite_index = pug.spr_bark{
sprite_index = spr_pigeon_coo;
image_speed = .25;
alarm[0] = floor(sprite_get_number(spr_pigeon_coo)/image_speed);
scr_sound(snd_duck_quack,g.sfx_volume);
action = 1;
speed = 0;
scr_score_point(g.min_score);
}}


//bite reaction
if scr_pug_dis(32){ 
if pug.sprite_index = pug.spr_poop || pug.sprite_index = pug.spr_bite{
if pug.pug_action = 6{
action = 1;
interact = "takeoff";
target = pug;
scr_score_point(g.small_score);
}}}



if action = 0{
if speed &gt; 0{
sprite_index = spr_pigeon_walk;
image_speed = .25;}
else{
sprite_index = spr_pigeon_stand;
image_single = 0;
image_speed = 0;}}


if distance_to_point(spawn_x,spawn_y) &gt; 8{
direction = point_direction(x,y,spawn_x,spawn_y);}

if speed &gt; .75{speed -= .75/30;}

}


//move around randomly
if action = 0 &amp;&amp; alarm[1] &lt;= 0 &amp;&amp; alarm[0] &lt;= 0{
alarm[1] = 60 + floor(random(60));
i = random(100);
if i &gt; 70{
alarm[0] = 30 + floor(random(30));
speed = 1;
if swim = 1{speed = 1.5;}else{
direction = random(359);
if distance_to_point(spawn_x,spawn_y) &gt; 32{
direction = point_direction(x,y,spawn_x,spawn_y);}}}
else{speed = 0;}

if i &lt; 10{
action = 1;
sprite_index = spr_pigeon_eat;
image_speed = .25;
alarm[0] = floor(sprite_get_number(sprite_index)/image_speed);}

if i &gt;= 10 &amp;&amp; i &lt; 13{
action = 1;
sprite_index = spr_pigeon_coo;
image_speed = .25;
alarm[0] = floor(sprite_get_number(sprite_index)/image_speed);
scr_sound(snd_pigeon_coo,g.sfx_volume);}
}



}//end if no interact
/////////////////////////////////////////////////////////

if interact = "takeoff"{
if spr_action = 0{
sprite_index = spr_pigeon_takeoff;
image_single = 0;
image_speed = .12;
speed = 0;
z = 0;
zspeed = 0;
if face = 1{direction = 180;}
else{direction = 0;}}
spr_action += image_speed;


if zspeed &lt; 2{zspeed += .025;}

if spr_action &gt;= sprite_get_number(sprite_index){
interact = "fly";
spr_action = 0;
sprite_index = spr_pigeon_fly;
image_speed = .12;
image_single = 0;
}}

if interact = "fly"{
image_speed = .25;
if speed &lt; 4{speed += .25;}
if zspeed &lt; 2{zspeed += .125;}

if x &lt; -32{x = -32;
interact = "wait offscreen";}
if x &gt; room_width+32{x = room_width+32;
interact = "wait offscreen";}}

if interact = "wait offscreen"{
speed = 0;
zspeed = 0;
interact = "return fly";
instance_create(target.x-16+random(32),target.y-16+random(32),obj_bird_poop);}

if interact = "return fly"{
direction = point_direction(x,y,spawn_x,spawn_y);
speed = clamp(distance_to_point(spawn_x,spawn_y)/5,.25,3);
z = distance_to_point(spawn_x,spawn_y)*.5;

if distance_to_point(spawn_x,spawn_y) &lt;= 32{
z = 0; speed = 0; 
interact = "";
action = 0;
alarm[0] = floor(random(30))+60;}

}




if interact = ""{
if flutter_immune &lt;= 0{
if scr_pug_dis(4) {
if pug.speed &gt;= 2{
interact = "flutter";
zspeed = irandom(5);
zgrav = 0.05 + (irandom(5)/100);
z = 1;
image_speed = .75;
direction = irandom(360);
speed = irandom(4)/4;

if pug.scoot &gt; 0{
scr_score_point(g.min_score);}

}}}}

if interact = "flutter"{
sprite_index = spr_pigeon_flutter;


if z &lt;= 0{interact = "";
zgrav = 0.05;
rot = 0;
speed = 0;
rotspeed = 0;
zspeed = 0;
flutter_immune = 15;}}

if flutter_immune &gt; 0{flutter_immune -= 1;}

//
}
