image_speed = 0;

//if releasing from pug mouth
if active = 1{
z = 9;
active = 0;
i = 2+g.frisbee_catch;
speed = 4+random(1+g.frisbee_catch)*.2;
if face = -1{direction = 0+random(i*2)-i;}else{direction = 180+random(i*2)-i;}}

if z &lt;= 0{
speed = 0;}

//collision
if !place_free(x+hspeed,y) || x-4 &lt; 0 || x+4 &gt; room_width{hspeed = hspeed*-.8;
if hspeed = 0{hspeed = 1;}}

if speed &gt; 0{speed = speed*.98;}


//unlock
if z &lt;= 1{
if scr_pug_dis(4) &gt; 0{
if pug.magic_mode = 0{
if g.unlock_frisbee = 0{
scr_score_point(points_worth);
g.unlock_frisbee = 1;
pug.spr_unlock = spr_unlock_frisbee;}}}}


//grab frisbee
if scr_pug_dis(8) &gt; 0{
if pug.sprite_index = pug.spr_bite || pug.sprite_index = pug.spr_bite_low{
if pug.item_held = ""{
pug.sprite_index = pug.spr_bite_low
if pug.pug_action = 1 &amp;&amp; pug.pug_action_proc = 0{
pug.pug_action_proc = 1;
pug.item_held = "Frisbee";
if z &gt;= 1{g.frisbee_catch += 1;pug.show_catch = 30;
scr_score_point(g.small_score*g.frisbee_catch);}
instance_destroy();
}}}}
