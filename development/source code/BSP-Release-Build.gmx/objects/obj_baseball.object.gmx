if pug.action = 5{pug.sprite_index = pug.spr_lowbite;}
if pug.action = 2{
pug.holding = "baseball";
instance_destroy();
}}}





//pee
if scr_pug_dis(96){ 
if pug.sprite_index = pug.spr_pee ||
pug.sprite_index = pug.spr_bark{
if pug.action = sprite_get_number(pug.spr_pee)-2{
roll = 25;
direction = point_direction(x,0,pug.x,0)+180;
speed = 3;}}}

if roll &gt; 0{
roll -= 1;
speed -= 3/25;

//colide
if x + hspeed &gt;= room_width - 16{hspeed = 0;}
if x + hspeed &lt;= 16{hspeed = 0;}
if !place_free(x+(hspeed*2),y){hspeed = 0;}
rot -= hspeed*3;
}

if roll &lt;= 1{
speed = 0;
gravity = 0;
hspeed = 0;}






}//end spawn
